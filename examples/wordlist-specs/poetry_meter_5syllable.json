{
  "name": "Poetry - 5 Syllable Words",
  "description": "Five-syllable words for poetry composition, particularly useful for haiku (5-7-5 pattern). Includes modern, literary words with reliable syllable counts.",
  "version": "1.0",
  "distribution": "en",
  "filters": {
    "syllables": {
      "exact": 5,
      "require_syllables": true
    },
    "labels": {
      "temporal": {
        "exclude": ["archaic", "obsolete"]
      },
      "register": {
        "exclude": ["vulgar", "offensive", "derogatory"]
      }
    },
    "phrase": {
      "is_phrase": false
    },
    "policy": {
      "modern_only": true
    }
  },
  "output": {
    "format": "wordlist",
    "sort": "alphabetical"
  },
  "metadata": {
    "target_audience": "Poets, writers, word enthusiasts",
    "use_cases": [
      "Haiku composition (5-7-5 syllable pattern)",
      "Poetry meter and rhythm",
      "Creative writing exercises",
      "Vocabulary building"
    ],
    "expected_count": "50-200 words",
    "examples": [
      "accidentally",
      "administrative",
      "communication",
      "electricity",
      "irregularly",
      "investigation",
      "mathematical",
      "philosophical",
      "pronunciation",
      "revolutionary"
    ],
    "notes": [
      "All words have exactly 5 syllables with Wiktionary verification",
      "Modern words only (no archaic terms)",
      "Single words only (no phrases)",
      "Perfect for haiku first or last lines"
    ]
  }
}
