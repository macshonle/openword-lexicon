{
  "version": "1.0",
  "name": "Scrabble Words",
  "description": "Single words suitable for Scrabble (no proper nouns)",
  "distribution": "en",
  "filters": {
    "phrase": {
      "max_words": 1
    },
    "frequency": {
      "min_tier": "S"
    },
    "proper_noun": {
      "require_common_usage": true
    }
  },
  "output": {
    "format": "text",
    "sort_by": "alphabetical"
  }
}
