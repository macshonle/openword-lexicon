{
  "version": "1.0",
  "name": "Wordle Words",
  "description": "5-letter common words for Wordle-style games",
  "distribution": "core",
  "filters": {
    "character": {
      "exact_length": 5,
      "pattern": "^[a-z]+$"
    },
    "phrase": {
      "max_words": 1
    },
    "frequency": {
      "min_tier": "Q"
    }
  },
  "output": {
    "format": "text",
    "sort_by": "alphabetical"
  }
}
