{
  "name": "wordlist-viewer",
  "version": "0.1.0",
  "description": "Interactive trie viewer for OpenWord Lexicon",
  "type": "module",
  "scripts": {
    "build-trie": "tsx src/build-trie.ts",
    "benchmark": "tsx src/benchmark-v6.ts",
    "dev": "http-server ../.. -p 8080 -o /web/viewer/index.html --cors",
    "start": "http-server ../.. -p 8080 -o /web/viewer/index.html --cors",
    "test": "tsx --test src/*.test.ts src/**/*.test.ts",
    "test:browser": "tsx src/browser-test.ts",
    "test:browser:v63": "tsx src/browser-test.ts --v63",
    "test:browser:all": "tsx src/browser-test.ts --all"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@types/node": "^20.10.0",
    "http-server": "^14.1.1",
    "playwright": "^1.57.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0"
  },
  "private": true,
  "dependencies": {
    "brotli-wasm": "^3.0.1"
  }
}
